-	CREATE_TABLE.EMPLOYE ( ID_EMPLOYEE VARCHAR(8) *PRIMARY KEY*, NAMA STRING(16), ID_DIVISI VARCHAR(2) *FOREIGN KEY*, STATUS STRING(5), NO_TELFON INTEGER(12), UMUR INTEGER(2), JENIS_KELAMIN STRING(1) )
-	CREATE_TABLE.DIVISI ( ID_DIVISI VARCHAR(4) *PRIMARY KEY*, DESKRIPSI STRING(20), KETERANGAN VARCHAR(50) )
